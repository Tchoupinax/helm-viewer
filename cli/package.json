{
  "name": "helm-viewer",
  "version": "0.5.0-rc.5",
  "description": "Quick tool to easily view your helm charts.",
  "author": "Tchoupinax <corentinfiloche@hotmail.fr> (https://corentinfiloche.xyz)",
  "license": "MIT",
  "bin": {
    "helm-viewer": "bin/helm-template.mjs"
  },
  "keywords": [
    "helm",
    "charts",
    "helm chart"
  ],
  "homepage": "https://github.com/Tchoupinax/helm-viewer",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tchoupinax/helm-viewer"
  },
  "bugs": {
    "url": "https://github.com/Tchoupinax/helm-viewer/issues"
  },
  "scripts": {
    "=== PUBLICATION ======================================": "",
    "pack": "npm pack --dry-run",
    "publish": "npm publish",
    "=== RUN ==============================================": "",
    "run": "NODE_ENV=development npx vite-node src/index.ts ~/Documents/swan/helm-charts/account-contract",
    "=== BUILD ============================================": "",
    "test": "vitest",
    "build": "tsc",
    "=== CLEAN ============================================": "",
    "clear": "rm -rf dist binary tmp compiled src/assets",
    "clean": "rm -rf dist binary tmp compiled src/assets"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.400.0",
    "chalk": "5.3.0",
    "crypto-js": "4.1.1",
    "execa": "8.0.1",
    "open": "8"
  },
  "devDependencies": {
    "@types/js-yaml": "4.0.5",
    "@types/node": "20.5.2",
    "@vercel/ncc": "0.36.1",
    "js-yaml": "4.1.0",
    "monaco-editor": "0.41.0",
    "npm-packlist": "7.0.4",
    "postject": "1.0.0-alpha.6",
    "typescript": "5.2.2",
    "vite-node": "0.34.3",
    "vitest": "0.34.3"
  }
}
